int& C_TEBeamSpline::m_nPoints( )
{
	static const auto offset = cheat::get_netvar_offset("C_TEBeamSpline", "m_nPoints");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

