bool& C_PhysicsProp::m_bAwake( )
{
	static const auto offset = cheat::get_netvar_offset("C_PhysicsProp", "m_bAwake");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

int& C_PhysicsProp::m_spawnflags( )
{
	static const auto offset = cheat::get_netvar_offset("C_PhysicsProp", "m_spawnflags");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

