bool& C_BRC4Target::m_bBrokenOpen( )
{
	static const auto offset = cheat::get_netvar_offset("C_BRC4Target", "m_bBrokenOpen");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

