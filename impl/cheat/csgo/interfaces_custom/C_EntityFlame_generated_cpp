cheat::csgo::CBaseHandle& C_EntityFlame::m_hEntAttached( )
{
	static const auto offset = cheat::get_netvar_offset("C_EntityFlame", "m_hEntAttached");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

