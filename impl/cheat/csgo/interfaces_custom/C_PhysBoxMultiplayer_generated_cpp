int& C_PhysBoxMultiplayer::m_iPhysicsMode( )
{
	static const auto offset = cheat::get_netvar_offset("C_PhysBoxMultiplayer", "m_iPhysicsMode");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

