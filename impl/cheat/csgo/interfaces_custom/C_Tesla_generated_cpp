char* C_Tesla::m_SoundName( )
{
	static const auto offset = cheat::get_netvar_offset("C_Tesla", "m_SoundName");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ptr( );
}

char* C_Tesla::m_iszSpriteName( )
{
	static const auto offset = cheat::get_netvar_offset("C_Tesla", "m_iszSpriteName");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ptr( );
}

