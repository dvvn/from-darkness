int& C_FireTrail::m_nAttachment( )
{
	static const auto offset = cheat::get_netvar_offset("C_FireTrail", "m_nAttachment");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

float& C_FireTrail::m_flLifetime( )
{
	static const auto offset = cheat::get_netvar_offset("C_FireTrail", "m_flLifetime");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

