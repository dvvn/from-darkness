int& C_TEBeamEnts::m_nStartEntity( )
{
	static const auto offset = cheat::get_netvar_offset("C_TEBeamEnts", "m_nStartEntity");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

