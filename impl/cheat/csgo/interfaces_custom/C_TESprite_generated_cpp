cheat::csgo::Vector& C_TESprite::m_vecOrigin( )
{
	static const auto offset = cheat::get_netvar_offset("C_TESprite", "m_vecOrigin");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

