bool& C_BaseTrigger::m_bClientSidePredicted( )
{
	static const auto offset = cheat::get_netvar_offset("C_BaseTrigger", "m_bClientSidePredicted");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

int& C_BaseTrigger::m_spawnflags( )
{
	static const auto offset = cheat::get_netvar_offset("C_BaseTrigger", "m_spawnflags");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

