char* C_FootstepControl::m_source( )
{
	static const auto offset = cheat::get_netvar_offset("C_FootstepControl", "m_source");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ptr( );
}

char* C_FootstepControl::m_destination( )
{
	static const auto offset = cheat::get_netvar_offset("C_FootstepControl", "m_destination");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ptr( );
}

