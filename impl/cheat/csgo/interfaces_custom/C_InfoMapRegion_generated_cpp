float& C_InfoMapRegion::m_flRadius( )
{
	static const auto offset = cheat::get_netvar_offset("C_InfoMapRegion", "m_flRadius");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ref( );
}

char* C_InfoMapRegion::m_szLocToken( )
{
	static const auto offset = cheat::get_netvar_offset("C_InfoMapRegion", "m_szLocToken");
	auto addr = nstd::mem::address(this).add(offset);
	return addr.ptr( );
}

