#include "C_ShadowControl.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_ShadowControl::m_shadowDirection( )
{
	static const auto offset = netvars::get( )->at("C_ShadowControl", "m_shadowDirection");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
int& C_ShadowControl::m_shadowColor( )
{
	static const auto offset = netvars::get( )->at("C_ShadowControl", "m_shadowColor");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
float& C_ShadowControl::m_flShadowMaxDist( )
{
	static const auto offset = netvars::get( )->at("C_ShadowControl", "m_flShadowMaxDist");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
bool& C_ShadowControl::m_bDisableShadows( )
{
	static const auto offset = netvars::get( )->at("C_ShadowControl", "m_bDisableShadows");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
bool& C_ShadowControl::m_bEnableLocalLightShadows( )
{
	static const auto offset = netvars::get( )->at("C_ShadowControl", "m_bEnableLocalLightShadows");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
