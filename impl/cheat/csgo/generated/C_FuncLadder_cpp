#include "C_FuncLadder.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_FuncLadder::m_vecPlayerMountPositionTop( )
{
	static const auto offset = netvars::get( )->at("C_FuncLadder", "m_vecPlayerMountPositionTop");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
cheat::csgo::Vector& C_FuncLadder::m_vecPlayerMountPositionBottom( )
{
	static const auto offset = netvars::get( )->at("C_FuncLadder", "m_vecPlayerMountPositionBottom");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
cheat::csgo::Vector& C_FuncLadder::m_vecLadderDir( )
{
	static const auto offset = netvars::get( )->at("C_FuncLadder", "m_vecLadderDir");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
bool& C_FuncLadder::m_bFakeLadder( )
{
	static const auto offset = netvars::get( )->at("C_FuncLadder", "m_bFakeLadder");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
