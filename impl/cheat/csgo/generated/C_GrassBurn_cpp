#include "C_GrassBurn.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

float& C_GrassBurn::m_flGrassBurnClearTime( )
{
	static const auto offset = netvars::get_ptr( )->at("C_GrassBurn", "m_flGrassBurnClearTime");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
