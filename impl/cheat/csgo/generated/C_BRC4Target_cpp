#include "C_BRC4Target.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

bool& C_BRC4Target::m_bBrokenOpen( )
{
	static const auto offset = netvars::get( )->at("C_BRC4Target", "m_bBrokenOpen");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
float& C_BRC4Target::m_flRadius( )
{
	static const auto offset = netvars::get( )->at("C_BRC4Target", "m_flRadius");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
