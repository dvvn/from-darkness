#include "C_Hostage.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

int& C_Hostage::m_isRescued( )
{
	static const auto offset = netvars::get( )->at("C_Hostage", "m_isRescued");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
int& C_Hostage::m_jumpedThisFrame( )
{
	static const auto offset = netvars::get( )->at("C_Hostage", "m_jumpedThisFrame");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
