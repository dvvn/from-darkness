#include "C_ItemAssaultSuitUseable.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_ItemAssaultSuitUseable::m_nArmorValue( )
{
	static const auto offset = netvars::get( )->at("C_ItemAssaultSuitUseable", "m_nArmorValue");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
bool& C_ItemAssaultSuitUseable::m_bIsHeavyAssaultSuit( )
{
	static const auto offset = netvars::get( )->at("C_ItemAssaultSuitUseable", "m_bIsHeavyAssaultSuit");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
