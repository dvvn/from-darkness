#include "C_EnvScreenOverlay.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include <array>

using namespace cheat::csgo;

std::array<char*, 10>& C_EnvScreenOverlay::m_iszOverlayNames( )
{
	static const auto offset = netvars::get( )->at("C_EnvScreenOverlay", "m_iszOverlayNames");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<char*, 10>>( );
}
std::array<float, 10>& C_EnvScreenOverlay::m_flOverlayTimes( )
{
	static const auto offset = netvars::get( )->at("C_EnvScreenOverlay", "m_flOverlayTimes");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<float, 10>>( );
}
float& C_EnvScreenOverlay::m_flStartTime( )
{
	static const auto offset = netvars::get( )->at("C_EnvScreenOverlay", "m_flStartTime");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
int& C_EnvScreenOverlay::m_iDesiredOverlay( )
{
	static const auto offset = netvars::get( )->at("C_EnvScreenOverlay", "m_iDesiredOverlay");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
bool& C_EnvScreenOverlay::m_bIsActive( )
{
	static const auto offset = netvars::get( )->at("C_EnvScreenOverlay", "m_bIsActive");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
