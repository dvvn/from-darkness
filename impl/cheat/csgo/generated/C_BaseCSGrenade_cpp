#include "C_BaseCSGrenade.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

bool& C_BaseCSGrenade::m_bRedraw( )
{
	static const auto offset = netvars::get( )->at("C_BaseCSGrenade", "m_bRedraw");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
bool& C_BaseCSGrenade::m_bIsHeldByPlayer( )
{
	static const auto offset = netvars::get( )->at("C_BaseCSGrenade", "m_bIsHeldByPlayer");
