#include "C_Precipitation.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_Precipitation::m_nPrecipType( )
{
	static const auto offset = netvars::get( )->at("C_Precipitation", "m_nPrecipType");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
