#include "C_ParticlePerformanceMonitor.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

bool& C_ParticlePerformanceMonitor::m_bMeasurePerf( )
{
	static const auto offset = netvars::get_ptr( )->at("C_ParticlePerformanceMonitor", "m_bMeasurePerf");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
bool& C_ParticlePerformanceMonitor::m_bDisplayPerf( )
{
	static const auto offset = netvars::get_ptr( )->at("C_ParticlePerformanceMonitor", "m_bDisplayPerf");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
