#include "C_RagdollManager.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_RagdollManager::m_iCurrentMaxRagdollCount( )
{
	static const auto offset = netvars::get( )->at("C_RagdollManager", "m_iCurrentMaxRagdollCount");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
