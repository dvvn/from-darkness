#include "C_WeaponM3.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_WeaponM3::m_reloadState( )
{
	static const auto offset = netvars::get_ptr( )->at("C_WeaponM3", "m_reloadState");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
