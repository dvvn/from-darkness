#include "C_BaseToggle.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_BaseToggle::m_vecFinalDest( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BaseToggle", "m_vecFinalDest");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
int& C_BaseToggle::m_movementType( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BaseToggle", "m_movementType");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
float& C_BaseToggle::m_flMoveTargetTime( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BaseToggle", "m_flMoveTargetTime");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
