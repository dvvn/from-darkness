#include "C_TEBeamRingPoint.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_TEBeamRingPoint::m_vecCenter( )
{
	static const auto offset = netvars::get( )->at("C_TEBeamRingPoint", "m_vecCenter");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
float& C_TEBeamRingPoint::m_flStartRadius( )
{
	static const auto offset = netvars::get( )->at("C_TEBeamRingPoint", "m_flStartRadius");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
float& C_TEBeamRingPoint::m_flEndRadius( )
{
	static const auto offset = netvars::get( )->at("C_TEBeamRingPoint", "m_flEndRadius");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
