#include "C_BaseTrigger.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

bool& C_BaseTrigger::m_bClientSidePredicted( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_BaseTrigger", "m_bClientSidePredicted");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
int& C_BaseTrigger::m_spawnflags( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_BaseTrigger", "m_spawnflags");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
