#include "C_TERadioIcon.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_TERadioIcon::m_iAttachToClient( )
{
	static const auto offset = netvars::get( )->at("C_TERadioIcon", "m_iAttachToClient");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
