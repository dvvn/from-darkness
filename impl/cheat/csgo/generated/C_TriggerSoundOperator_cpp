#include "C_TriggerSoundOperator.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_TriggerSoundOperator::m_nSoundOperator( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TriggerSoundOperator", "m_nSoundOperator");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
