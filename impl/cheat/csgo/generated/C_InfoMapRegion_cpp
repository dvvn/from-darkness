#include "C_InfoMapRegion.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

float& C_InfoMapRegion::m_flRadius( )
{
	static const auto offset = netvars::get( )->at("C_InfoMapRegion", "m_flRadius");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
char* C_InfoMapRegion::m_szLocToken( )
{
	static const auto offset = netvars::get( )->at("C_InfoMapRegion", "m_szLocToken");
	auto addr = nstd::address(this).add(offset);
	return addr.ptr<char>( );
}
