#include "C_VGuiScreen.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/CBaseHandle.hpp"

using namespace cheat::csgo;

float& C_VGuiScreen::m_flWidth( )
{
	static const auto offset = netvars::get( )->at("C_VGuiScreen", "m_flWidth");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
float& C_VGuiScreen::m_flHeight( )
{
	static const auto offset = netvars::get( )->at("C_VGuiScreen", "m_flHeight");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
