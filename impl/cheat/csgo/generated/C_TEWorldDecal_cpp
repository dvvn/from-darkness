#include "C_TEWorldDecal.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_TEWorldDecal::m_vecOrigin( )
{
	static const auto offset = netvars::get( )->at("C_TEWorldDecal", "m_vecOrigin");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
int& C_TEWorldDecal::m_nIndex( )
{
	static const auto offset = netvars::get( )->at("C_TEWorldDecal", "m_nIndex");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
