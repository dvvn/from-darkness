#include "C_TEEnergySplash.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector& C_TEEnergySplash::m_vecPos( )
{
	static const auto offset = netvars::get( )->at("C_TEEnergySplash", "m_vecPos");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
cheat::csgo::Vector& C_TEEnergySplash::m_vecDir( )
{
	static const auto offset = netvars::get( )->at("C_TEEnergySplash", "m_vecDir");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
bool& C_TEEnergySplash::m_bExplosive( )
{
	static const auto offset = netvars::get( )->at("C_TEEnergySplash", "m_bExplosive");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
