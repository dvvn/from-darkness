#include "C_TestTraceline.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Color.hpp"
#include "cheat/csgo/QAngle.hpp"
#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

cheat::csgo::Color& C_TestTraceline::m_clrRender( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TestTraceline", "m_clrRender");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Color>( );
}
cheat::csgo::Vector& C_TestTraceline::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TestTraceline", "m_vecOrigin");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
cheat::csgo::QAngle& C_TestTraceline::m_angRotation( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TestTraceline", "m_angRotation");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::QAngle>( );
}
int& C_TestTraceline::moveparent( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TestTraceline", "moveparent");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
