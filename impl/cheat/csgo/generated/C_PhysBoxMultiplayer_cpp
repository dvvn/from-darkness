#include "C_PhysBoxMultiplayer.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_PhysBoxMultiplayer::m_iPhysicsMode( )
{
	static const auto offset = netvars::get_ptr( )->at("C_PhysBoxMultiplayer", "m_iPhysicsMode");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
float& C_PhysBoxMultiplayer::m_fMass( )
{
	static const auto offset = netvars::get_ptr( )->at("C_PhysBoxMultiplayer", "m_fMass");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
