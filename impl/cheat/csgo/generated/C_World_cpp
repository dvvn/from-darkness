#include "C_World.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include "cheat/csgo/Vector.hpp"

using namespace cheat::csgo;

float& C_World::m_flWaveHeight( )
{
	static const auto offset = netvars::get( )->at("C_World", "m_flWaveHeight");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
cheat::csgo::Vector& C_World::m_WorldMins( )
{
	static const auto offset = netvars::get( )->at("C_World", "m_WorldMins");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector>( );
}
