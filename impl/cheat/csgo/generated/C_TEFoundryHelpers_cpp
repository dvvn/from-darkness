#include "C_TEFoundryHelpers.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_TEFoundryHelpers::m_iEntity( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEFoundryHelpers", "m_iEntity");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
