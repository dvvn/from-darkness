using namespace cheat::csgo;

int& C_BaseAnimating::m_nSequence( )
{
	static const auto offset = netvars::get( )->at("C_BaseAnimating", "m_nSequence");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
