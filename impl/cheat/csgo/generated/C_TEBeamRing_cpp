#include "C_TEBeamRing.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_TEBeamRing::m_nStartEntity( )
{
	static const auto offset = netvars::get( )->at("C_TEBeamRing", "m_nStartEntity");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
int& C_TEBeamRing::m_nEndEntity( )
{
	static const auto offset = netvars::get( )->at("C_TEBeamRing", "m_nEndEntity");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
