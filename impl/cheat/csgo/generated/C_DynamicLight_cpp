#include "C_DynamicLight.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_DynamicLight::m_Flags( )
{
	static const auto offset = netvars::get( )->at("C_DynamicLight", "m_Flags");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
int& C_DynamicLight::m_LightStyle( )
{
	static const auto offset = netvars::get( )->at("C_DynamicLight", "m_LightStyle");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
