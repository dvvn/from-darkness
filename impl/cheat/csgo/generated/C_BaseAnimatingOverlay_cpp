#include "C_BaseAnimatingOverlay.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include <array>

using namespace cheat::csgo;

std::array<_ST_m_AnimOverlay_15, 15>& C_BaseAnimatingOverlay::m_AnimOverlay( )
{
	static const auto offset = netvars::get( )->at("C_BaseAnimatingOverlay", "m_AnimOverlay");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<_ST_m_AnimOverlay_15, 15>>( );
}
