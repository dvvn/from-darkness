#include "C_WeaponNOVA.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_WeaponNOVA::m_reloadState( )
{
	static const auto offset = netvars::get_ptr( )->at("C_WeaponNOVA", "m_reloadState");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
