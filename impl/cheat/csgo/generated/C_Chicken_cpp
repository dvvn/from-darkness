#include "C_Chicken.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

int& C_Chicken::m_jumpedThisFrame( )
{
	static const auto offset = netvars::get( )->at("C_Chicken", "m_jumpedThisFrame");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
int& C_Chicken::m_leader( )
{
	static const auto offset = netvars::get( )->at("C_Chicken", "m_leader");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
