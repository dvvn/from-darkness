#include "C_Chicken.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

int& C_Chicken::m_jumpedThisFrame( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Chicken", "m_jumpedThisFrame");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
int& C_Chicken::m_leader( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Chicken", "m_leader");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
