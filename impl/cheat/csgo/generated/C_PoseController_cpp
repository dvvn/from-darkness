#include "C_PoseController.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

#include <array>

using namespace cheat::csgo;

std::array<int, 4>& C_PoseController::m_hProps( )
{
	static const auto offset = netvars::get( )->at("C_PoseController", "m_hProps");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<int, 4>>( );
}
std::array<int, 4>& C_PoseController::m_chPoseIndex( )
{
	static const auto offset = netvars::get( )->at("C_PoseController", "m_chPoseIndex");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<int, 4>>( );
}
