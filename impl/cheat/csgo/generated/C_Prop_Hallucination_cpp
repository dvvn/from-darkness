#include "C_Prop_Hallucination.h"
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>

using namespace cheat::csgo;

bool& C_Prop_Hallucination::m_bEnabled( )
{
	static const auto offset = netvars::get( )->at("C_Prop_Hallucination", "m_bEnabled");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
}
float& C_Prop_Hallucination::m_fVisibleTime( )
{
	static const auto offset = netvars::get( )->at("C_Prop_Hallucination", "m_fVisibleTime");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
float& C_Prop_Hallucination::m_fRechargeTime( )
{
	static const auto offset = netvars::get( )->at("C_Prop_Hallucination", "m_fRechargeTime");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
}
