int& C_BaseCombatWeapon::m_iPrimaryAmmoType( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BaseCombatWeapon", "m_iPrimaryAmmoType");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
}
