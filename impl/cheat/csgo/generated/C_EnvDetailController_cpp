#include "C_EnvDetailController.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

float& C_EnvDetailController::m_flFadeStartDist( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(float*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EnvDetailController", "m_flFadeStartDist");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
#endif
}
float& C_EnvDetailController::m_flFadeEndDist( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(float*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EnvDetailController", "m_flFadeEndDist");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<float>( );
#endif
}
