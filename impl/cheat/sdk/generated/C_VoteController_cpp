bool& C_VoteController::m_bIsYesNoVote( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_VoteController", "m_bIsYesNoVote");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
