int32_t& C_ItemAssaultSuitUseable::m_nArmorValue( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_ItemAssaultSuitUseable.m_nArmorValue");
	auto addr = utl::address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
bool& C_ItemAssaultSuitUseable::m_bIsHeavyAssaultSuit( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_ItemAssaultSuitUseable.m_bIsHeavyAssaultSuit");
	auto addr = utl::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
