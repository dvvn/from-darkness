#include "C_PostProcessController.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include <array>

using namespace cheat;
using namespace csgo;

bool& C_PostProcessController::m_bMaster( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_PostProcessController", "m_bMaster");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
std::array<float, 11>& C_PostProcessController::m_flPostProcessParameters( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(std::array<float, 11>*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_PostProcessController", "m_flPostProcessParameters");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<float, 11>>( );
#endif
}
