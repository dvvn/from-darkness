#include "C_ItemDogtags.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_ItemDogtags::m_OwningPlayer( )
{
	static const auto offset = netvars::get_ptr( )->at("C_ItemDogtags.m_OwningPlayer");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
int32_t& C_ItemDogtags::m_KillingPlayer( )
{
	static const auto offset = netvars::get_ptr( )->at("C_ItemDogtags.m_KillingPlayer");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
