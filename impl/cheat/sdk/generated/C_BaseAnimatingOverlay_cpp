#include "C_BaseAnimatingOverlay.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include <array>

using namespace cheat;
using namespace csgo;

std::array<_ST_m_AnimOverlay_15, 15>& C_BaseAnimatingOverlay::m_AnimOverlay( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(std::array<_ST_m_AnimOverlay_15, 15>*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_BaseAnimatingOverlay", "m_AnimOverlay");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<_ST_m_AnimOverlay_15, 15>>( );
#endif
}
