#include "C_EnvParticleScript.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

float& C_EnvParticleScript::m_flSequenceScale( )
{
	static const auto offset = netvars::get_ptr( )->at("C_EnvParticleScript.m_flSequenceScale");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
