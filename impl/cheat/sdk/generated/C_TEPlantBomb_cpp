#include "C_TEPlantBomb.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector& C_TEPlantBomb::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEPlantBomb.m_vecOrigin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
int32_t& C_TEPlantBomb::m_iPlayer( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEPlantBomb.m_iPlayer");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
int32_t& C_TEPlantBomb::m_option( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEPlantBomb.m_option");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
