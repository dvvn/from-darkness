#include "C_FuncConveyor.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

float& C_FuncConveyor::m_flConveyorSpeed( )
{
	static const auto offset = netvars::get_ptr( )->at("C_FuncConveyor.m_flConveyorSpeed");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
