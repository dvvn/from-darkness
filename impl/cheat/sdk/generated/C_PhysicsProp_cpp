#include "C_PhysicsProp.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

bool& C_PhysicsProp::m_bAwake( )
{
	static const auto offset = netvars::get_ptr( )->at("C_PhysicsProp.m_bAwake");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
int32_t& C_PhysicsProp::m_spawnflags( )
{
	static const auto offset = netvars::get_ptr( )->at("C_PhysicsProp.m_spawnflags");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
