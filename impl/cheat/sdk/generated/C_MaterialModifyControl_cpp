char* C_MaterialModifyControl::m_szMaterialName( )
{
	static const auto offset = netvars::get_ptr( )->at("C_MaterialModifyControl.m_szMaterialName");
	auto addr = utl::mem::address(this).add(offset);
	return addr.raw<char>( );
}
