float& C_FireSmoke::m_flStartScale( )
{
	static const auto offset = netvars::get_ptr( )->at("C_FireSmoke.m_flStartScale");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
