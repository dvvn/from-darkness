float& C_WaterLODControl::m_flCheapWaterStartDistance( )
{
	static const auto offset = netvars::get_ptr( )->at("C_WaterLODControl.m_flCheapWaterStartDistance");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
