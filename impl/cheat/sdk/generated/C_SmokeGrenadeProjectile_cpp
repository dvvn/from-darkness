#include "C_SmokeGrenadeProjectile.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

bool& C_SmokeGrenadeProjectile::m_bDidSmokeEffect( )
{
	static const auto offset = netvars::get_ptr( )->at("C_SmokeGrenadeProjectile.m_bDidSmokeEffect");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
int32_t& C_SmokeGrenadeProjectile::m_nSmokeEffectTickBegin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_SmokeGrenadeProjectile.m_nSmokeEffectTickBegin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
