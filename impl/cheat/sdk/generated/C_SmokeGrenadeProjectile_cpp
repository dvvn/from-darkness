#include "C_SmokeGrenadeProjectile.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

bool& C_SmokeGrenadeProjectile::m_bDidSmokeEffect( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_SmokeGrenadeProjectile", "m_bDidSmokeEffect");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
int& C_SmokeGrenadeProjectile::m_nSmokeEffectTickBegin( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_SmokeGrenadeProjectile", "m_nSmokeEffectTickBegin");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
