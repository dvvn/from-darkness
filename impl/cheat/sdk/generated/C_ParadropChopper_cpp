#include "C_ParadropChopper.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include "cheat/sdk/CBaseHandle.hpp"
#include "cheat/sdk/Vector2D.hpp"

using namespace cheat::csgo;

cheat::csgo::Vector2D& C_ParadropChopper::m_vecOrigin( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(cheat::csgo::Vector2D*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_ParadropChopper", "m_vecOrigin");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::Vector2D>( );
#endif
}
cheat::csgo::CBaseHandle& C_ParadropChopper::m_hCallingPlayer( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(cheat::csgo::CBaseHandle*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_ParadropChopper", "m_hCallingPlayer");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::CBaseHandle>( );
#endif
}
