#include "C_Inferno.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

std::array<int32_t, 100>& C_Inferno::m_bFireIsBurning( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(std::array<int32_t, 100>*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Inferno", "m_bFireIsBurning");
	auto addr = address(this).add(offset);
	return addr.ref<std::array<int32_t, 100>>( );
#endif
}
