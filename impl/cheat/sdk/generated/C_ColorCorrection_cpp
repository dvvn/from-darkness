utl::Vector& C_ColorCorrection::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_ColorCorrection.m_vecOrigin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
