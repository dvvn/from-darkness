utl::Vector& C_TEBeamRingPoint::m_vecCenter( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamRingPoint.m_vecCenter");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
