bool& C_PointCommentaryNode::m_bActive( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_PointCommentaryNode.m_bActive");
	auto addr = utl::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
