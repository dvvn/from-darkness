#include "C_FootstepControl.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

char* C_FootstepControl::m_source( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return (char*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_FootstepControl", "m_source");
	auto addr = nstd::address(this).add(offset);
	return addr.ptr<char>( );
#endif
}
char* C_FootstepControl::m_destination( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return (char*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_FootstepControl", "m_destination");
	auto addr = nstd::address(this).add(offset);
	return addr.ptr<char>( );
#endif
}
