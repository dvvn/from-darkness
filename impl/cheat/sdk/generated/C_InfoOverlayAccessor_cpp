#include "C_InfoOverlayAccessor.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

int32_t& C_InfoOverlayAccessor::m_iOverlayID( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_InfoOverlayAccessor", "m_iOverlayID");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
int32_t& C_InfoOverlayAccessor::m_iTextureFrameIndex( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_InfoOverlayAccessor", "m_iTextureFrameIndex");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
