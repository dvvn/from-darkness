#include "C_InfoOverlayAccessor.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

int& C_InfoOverlayAccessor::m_iOverlayID( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_InfoOverlayAccessor", "m_iOverlayID");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
int& C_InfoOverlayAccessor::m_iTextureFrameIndex( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_InfoOverlayAccessor", "m_iTextureFrameIndex");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
