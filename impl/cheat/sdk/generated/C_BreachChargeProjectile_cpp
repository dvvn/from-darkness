bool& C_BreachChargeProjectile::m_bShouldExplode( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BreachChargeProjectile.m_bShouldExplode");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
