#include "C_Fists.h"

using namespace cheat;
using namespace csgo;

bool& C_Fists::m_bPlayingUninterruptableAct( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Fists", "m_bPlayingUninterruptableAct");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
