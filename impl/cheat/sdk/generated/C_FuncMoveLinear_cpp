#include "C_FuncMoveLinear.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

int32_t& C_FuncMoveLinear::m_fFlags( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_FuncMoveLinear", "m_fFlags");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
Vector& C_FuncMoveLinear::m_vecVelocity( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(Vector*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_FuncMoveLinear", "m_vecVelocity");
	auto addr = address(this).add(offset);
	return addr.ref<Vector>( );
#endif
}
