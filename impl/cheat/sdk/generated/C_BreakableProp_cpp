#include "C_BreakableProp.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::QAngle& C_BreakableProp::m_qPreferredPlayerCarryAngles( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BreakableProp.m_qPreferredPlayerCarryAngles");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::QAngle>( );
}
bool& C_BreakableProp::m_bClientPhysics( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BreakableProp.m_bClientPhysics");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
