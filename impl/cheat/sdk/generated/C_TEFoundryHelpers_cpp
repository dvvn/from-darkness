#include "C_TEFoundryHelpers.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_TEFoundryHelpers::m_iEntity( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEFoundryHelpers.m_iEntity");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
