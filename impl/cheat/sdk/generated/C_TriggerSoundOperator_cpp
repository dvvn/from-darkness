#include "C_TriggerSoundOperator.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

int32_t& C_TriggerSoundOperator::m_nSoundOperator( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TriggerSoundOperator", "m_nSoundOperator");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
