#include "C_TERadioIcon.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_TERadioIcon::m_iAttachToClient( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TERadioIcon.m_iAttachToClient");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
