#include "C_MolotovProjectile.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

bool& C_MolotovProjectile::m_bIsIncGrenade( )
{
	static const auto offset = netvars::get_ptr( )->at("C_MolotovProjectile.m_bIsIncGrenade");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
