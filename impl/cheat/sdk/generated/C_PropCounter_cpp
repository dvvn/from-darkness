#include "C_PropCounter.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

float& C_PropCounter::m_flDisplayValue( )
{
	static const auto offset = netvars::get_ptr( )->at("C_PropCounter.m_flDisplayValue");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
