#include "C_Chicken.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_Chicken::m_jumpedThisFrame( )
{
	static const auto offset = netvars::get_ptr( )->at("C_Chicken.m_jumpedThisFrame");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
int32_t& C_Chicken::m_leader( )
{
	static const auto offset = netvars::get_ptr( )->at("C_Chicken.m_leader");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
