#include "C_BaseFlex.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace csgo;

utl::array<float, 96>& C_BaseFlex::m_flexWeight( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(utl::array<float, 96>*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_BaseFlex.m_flexWeight");
	auto addr = utl::address(this).add(offset);
	return addr.ref<utl::array<float, 96>>( );
#endif
}
