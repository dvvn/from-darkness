#include "C_TEDust.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

float& C_TEDust::m_flSize( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(float*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEDust", "m_flSize");
	auto addr = address(this).add(offset);
	return addr.ref<float>( );
#endif
}
float& C_TEDust::m_flSpeed( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(float*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEDust", "m_flSpeed");
	auto addr = address(this).add(offset);
	return addr.ref<float>( );
#endif
}
Vector& C_TEDust::m_vecDirection( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(Vector*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEDust", "m_vecDirection");
	auto addr = address(this).add(offset);
	return addr.ref<Vector>( );
#endif
}
