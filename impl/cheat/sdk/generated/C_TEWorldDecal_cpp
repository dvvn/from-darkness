#include "C_TEWorldDecal.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector& C_TEWorldDecal::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEWorldDecal.m_vecOrigin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
int32_t& C_TEWorldDecal::m_nIndex( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEWorldDecal.m_nIndex");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
