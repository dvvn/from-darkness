bool& C_EnvDOFController::m_bDOFEnabled( )
{
	static const auto offset = netvars::get_ptr( )->at("C_EnvDOFController.m_bDOFEnabled");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
