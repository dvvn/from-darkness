#include "C_RagdollManager.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

int& C_RagdollManager::m_iCurrentMaxRagdollCount( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_RagdollManager", "m_iCurrentMaxRagdollCount");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
