#include "C_RagdollManager.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_RagdollManager::m_iCurrentMaxRagdollCount( )
{
	static const auto offset = netvars::get_ptr( )->at("C_RagdollManager.m_iCurrentMaxRagdollCount");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
