#include "C_WeaponCSBaseGun.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

using namespace cheat::csgo;

int& C_WeaponCSBaseGun::m_iBurstShotsRemaining( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_WeaponCSBaseGun", "m_iBurstShotsRemaining");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
int& C_WeaponCSBaseGun::m_zoomLevel( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_WeaponCSBaseGun", "m_zoomLevel");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int>( );
#endif
}
