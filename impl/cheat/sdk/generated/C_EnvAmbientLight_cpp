#include "C_EnvAmbientLight.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector& C_EnvAmbientLight::m_vecColor( )
{
	static const auto offset = netvars::get_ptr( )->at("C_EnvAmbientLight.m_vecColor");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
