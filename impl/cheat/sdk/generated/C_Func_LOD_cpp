int32_t& C_Func_LOD::m_nDisappearMinDist( )
{
	static const auto offset = netvars::get_ptr( )->at("C_Func_LOD.m_nDisappearMinDist");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
