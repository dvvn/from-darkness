#include "C_Func_LOD.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include <cstdint>

using namespace cheat;
using namespace csgo;

int32_t& C_Func_LOD::m_nDisappearMaxDist( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Func_LOD", "m_nDisappearMaxDist");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
int32_t& C_Func_LOD::m_nDisappearMinDist( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_Func_LOD", "m_nDisappearMinDist");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
