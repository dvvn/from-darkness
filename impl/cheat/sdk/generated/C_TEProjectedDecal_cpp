#include "C_TEProjectedDecal.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector& C_TEProjectedDecal::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEProjectedDecal.m_vecOrigin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
