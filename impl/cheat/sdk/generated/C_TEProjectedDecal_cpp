	return addr.ref<utl::QAngle>( );
}
float& C_TEProjectedDecal::m_flDistance( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEProjectedDecal.m_flDistance");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
int32_t& C_TEProjectedDecal::m_nIndex( )
