#include "C_TEMetalSparks.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

Vector& C_TEMetalSparks::m_vecDir( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(Vector*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEMetalSparks", "m_vecDir");
	auto addr = address(this).add(offset);
	return addr.ref<Vector>( );
#endif
}
Vector& C_TEMetalSparks::m_vecPos( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(Vector*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEMetalSparks", "m_vecPos");
	auto addr = address(this).add(offset);
	return addr.ref<Vector>( );
#endif
}
