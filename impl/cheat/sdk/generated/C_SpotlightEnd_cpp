#include "C_SpotlightEnd.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

float& C_SpotlightEnd::m_flLightScale( )
{
	static const auto offset = netvars::get_ptr( )->at("C_SpotlightEnd.m_flLightScale");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
float& C_SpotlightEnd::m_Radius( )
{
	static const auto offset = netvars::get_ptr( )->at("C_SpotlightEnd.m_Radius");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
