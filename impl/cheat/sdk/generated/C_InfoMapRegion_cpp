#include "C_InfoMapRegion.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

float& C_InfoMapRegion::m_flRadius( )
{
	static const auto offset = netvars::get_ptr( )->at("C_InfoMapRegion.m_flRadius");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<float>( );
}
char* C_InfoMapRegion::m_szLocToken( )
{
	static const auto offset = netvars::get_ptr( )->at("C_InfoMapRegion.m_szLocToken");
	auto addr = utl::mem::address(this).add(offset);
	return addr.raw<char>( );
}
