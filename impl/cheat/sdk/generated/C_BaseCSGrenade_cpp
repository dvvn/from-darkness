bool& C_BaseCSGrenade::m_bRedraw( )
{
	static const auto offset = netvars::get_ptr( )->at("C_BaseCSGrenade.m_bRedraw");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
