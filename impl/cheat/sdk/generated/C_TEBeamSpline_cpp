#include "C_TEBeamSpline.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include <array>
#include <cstdint>

using namespace cheat;
using namespace csgo;

int32_t& C_TEBeamSpline::m_nPoints( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamSpline", "m_nPoints");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
std::array<Vector, 16>& C_TEBeamSpline::m_vecPoints( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(std::array<Vector, 16>*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamSpline", "m_vecPoints");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<std::array<Vector, 16>>( );
#endif
}
