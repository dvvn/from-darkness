#include "C_TEBeamSpline.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

int32_t& C_TEBeamSpline::m_nPoints( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamSpline.m_nPoints");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
utl::array<utl::Vector, 16>& C_TEBeamSpline::m_vecPoints( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamSpline.m_vecPoints");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::array<utl::Vector, 16>>( );
}
