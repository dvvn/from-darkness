#include "C_TEBeamEnts.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

int32_t& C_TEBeamEnts::m_nEndEntity( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamEnts", "m_nEndEntity");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
int32_t& C_TEBeamEnts::m_nStartEntity( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEBeamEnts", "m_nStartEntity");
	auto addr = address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
