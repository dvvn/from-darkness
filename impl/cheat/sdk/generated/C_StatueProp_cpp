int32_t& C_StatueProp::m_hInitBaseAnimating( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(int32_t*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_StatueProp.m_hInitBaseAnimating");
	auto addr = utl::address(this).add(offset);
	return addr.ref<int32_t>( );
#endif
}
