#include "C_TEEnergySplash.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector& C_TEEnergySplash::m_vecPos( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEEnergySplash.m_vecPos");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
utl::Vector& C_TEEnergySplash::m_vecDir( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEEnergySplash.m_vecDir");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
bool& C_TEEnergySplash::m_bExplosive( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEEnergySplash.m_bExplosive");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<bool>( );
}
