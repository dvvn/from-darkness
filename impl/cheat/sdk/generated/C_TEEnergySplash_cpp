#include "C_TEEnergySplash.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

bool& C_TEEnergySplash::m_bExplosive( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_TEEnergySplash", "m_bExplosive");
	auto addr = address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
