int32_t& C_TEGaussExplosion::m_nType( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEGaussExplosion.m_nType");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<int32_t>( );
}
utl::Vector& C_TEGaussExplosion::m_vecDirection( )
{
	static const auto offset = netvars::get_ptr( )->at("C_TEGaussExplosion.m_vecDirection");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector>( );
}
