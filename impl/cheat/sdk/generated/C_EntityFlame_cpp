#include "C_EntityFlame.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

bool& C_EntityFlame::m_bCheapEffect( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EntityFlame", "m_bCheapEffect");
	auto addr = address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
CBaseHandle& C_EntityFlame::m_hEntAttached( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(CBaseHandle*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EntityFlame", "m_hEntAttached");
	auto addr = address(this).add(offset);
	return addr.ref<CBaseHandle>( );
#endif
}
