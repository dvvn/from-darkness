#include "C_EntityFlame.h"
#include "cheat/netvars/config.h"
#ifndef CHEAT_NETVARS_UPDATING
#include "cheat/netvars/netvars.h"
#include <nstd/address.h>
#endif

#include "cheat/sdk/CBaseHandle.hpp"

using namespace cheat::csgo;

bool& C_EntityFlame::m_bCheapEffect( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(bool*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EntityFlame", "m_bCheapEffect");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<bool>( );
#endif
}
cheat::csgo::CBaseHandle& C_EntityFlame::m_hEntAttached( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(cheat::csgo::CBaseHandle*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_EntityFlame", "m_hEntAttached");
	auto addr = nstd::address(this).add(offset);
	return addr.ref<cheat::csgo::CBaseHandle>( );
#endif
}
