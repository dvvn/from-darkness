#include "C_SurvivalSpawnChopper.h"

#include "cheat/netvars/netvars.h"

using namespace cheat;
using namespace utl;
using namespace csgo;

Vector2D& C_SurvivalSpawnChopper::m_vecOrigin( )
{
#ifdef CHEAT_NETVARS_UPDATING
	return *(Vector2D*)nullptr;
#else
	static const auto offset = netvars::get_ptr( )->at("C_SurvivalSpawnChopper", "m_vecOrigin");
	auto addr = address(this).add(offset);
	return addr.ref<Vector2D>( );
#endif
}
