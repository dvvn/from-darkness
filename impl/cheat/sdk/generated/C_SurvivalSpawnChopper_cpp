#include "C_SurvivalSpawnChopper.h"

#include "cheat/core/netvars.h"

using namespace cheat;
using namespace csgo;

utl::Vector2D& C_SurvivalSpawnChopper::m_vecOrigin( )
{
	static const auto offset = netvars::get_ptr( )->at("C_SurvivalSpawnChopper.m_vecOrigin");
	auto addr = utl::mem::address(this).add(offset);
	return addr.ref<utl::Vector2D>( );
}
